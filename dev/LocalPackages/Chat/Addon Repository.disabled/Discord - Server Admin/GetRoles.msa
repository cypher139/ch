#version 1.0-tmc
admin:/discordgr [$type] [$id] [$] = >>>
@name = 'Discord'
@roles = ''
if($id == '' || $id == null) {
	die('Specify the Discord user ID!')
}
switch($type) {
case 0:
case 'discord':
case 'chd':
	try {
		@roles = discord_member_get_roles($id)
	} catch(Exception @ex) {
		die('ID not found '.@ex['message'])
	}
case 1:
case 'saved':
case 'gv':
	@roles = get_value('server.Discord.members.roles')
	if(is_array(@roles)) {
		if(array_index_exists(@roles, $id)) {
			@roles = @roles[$id]
		} else {
			die('ID not found')
		}
	}
default:
	die('Specify Search type: CHDiscord cache or internal storage')
}
msg('Roles found: '.@roles)
<<<