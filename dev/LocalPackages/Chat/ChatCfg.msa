#version 1.1-tmc
admin:/chatcfg [$type='Server'] [$] = >>>
proc _chat_help() {
	msg(color(6).'Chat: Admin help:')
	msg(color('a').'Command usage: <required> [optional]')
	msg(color('a').'/chatcfg <action> [option1] [option2] [option3] [option4]')
	msg(color(6).'Actions:')
	msg(color(7).'/chatcfg <config>'.color(4).' - '.color('f').'Allows you to set global chat format options, such as colorizing color codes in chat, and more!')

}
if($type == 'help') { _chat_help() die() }

@prev = import('Chat.config.'.$type)

@ret = _chat_load_config($type)
if(is_array(@ret)) { 
	if(is_array(@prev)) { @loadtype = 'Reloaded' } else { @loadtype = 'Loaded' }
	msg('[Chat] '.color(10).@loadtype.' config: '.color(7).$type)
} else {
	msg('[Chat] '.color(12).'Failed to load config: '.color(7).$type)
}
<<<